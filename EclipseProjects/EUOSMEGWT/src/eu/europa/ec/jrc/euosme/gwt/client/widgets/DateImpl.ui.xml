<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:widgets="urn:import:eu.europa.ec.jrc.euosme.gwt.client">
	<g:DisclosurePanel ui:field="componentPanel">
 		<g:customHeader>
   			<g:Label addStyleNames="h3" ui:field="myLabel"/>     				
   		</g:customHeader> 		
 		<g:VerticalPanel>
 			<g:HorizontalPanel>
	   			<widgets:MyDateBox addStyleNames="svP" ui:field="myDateBox"/>
	   			<g:Button addStyleNames="infoButton" ui:field="infoButton"/>
	   		</g:HorizontalPanel> 			
 			<g:Label styleName="error" ui:field="myError"/>
 		</g:VerticalPanel>
 	</g:DisclosurePanel>		
</ui:UiBinder> 