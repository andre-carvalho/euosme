<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DisclosurePanel ui:field="componentPanel">
 		<g:customHeader>   			
   			<g:HorizontalPanel>
				<g:Label addStyleNames="h3" ui:field="myLabel"/>    
	   			<g:Button addStyleNames="infoButton" ui:field="infoButton"/>	   		
			</g:HorizontalPanel> 				
   		</g:customHeader> 		
 		<g:VerticalPanel addStyleNames="svP">
			<g:FlexTable ui:field="myFlexTable"/>
			<g:ListBox ui:field="myListBox"/>
			<g:HorizontalPanel>
				<g:TextBox ui:field="newTextBox" />
				<g:Button ui:field="newButton" />	   			   		
			</g:HorizontalPanel>
			<g:Label styleName="error" ui:field="myError"/> 				
		</g:VerticalPanel>
 	</g:DisclosurePanel>	
</ui:UiBinder> 