<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:DisclosurePanel ui:field="componentPanel">
 		<g:customHeader>
   			<g:Label addStyleNames="h3" ui:field="myLabel"/>     				
   		</g:customHeader> 		
 		<g:VerticalPanel>
 			<g:HorizontalPanel>
 				<g:HorizontalPanel ui:field="myHP"/>
	   			<g:Button addStyleNames="infoButton" ui:field="infoButton"/>
	   		</g:HorizontalPanel>
	   		<g:Label ui:field="labelList"/>
 			<g:Label styleName="error" ui:field="myError"/>
 		</g:VerticalPanel>
 	</g:DisclosurePanel>	
</ui:UiBinder> 