<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel ui:field="myPanel">
		<g:DisclosurePanel ui:field="componentPanel">
	 		<g:customHeader>
	   			<g:Label addStyleNames="h3" ui:field="myLabel"/>   			    				
	   		</g:customHeader>
	 		<g:VerticalPanel>
	 			<g:VerticalPanel ui:field="fieldsGroup"/>
	 			<g:Label styleName="error" ui:field="myError"/>
	 			<g:Button ui:field="removeGroupButton" visible="false"/>							
			</g:VerticalPanel>	 				
		</g:DisclosurePanel>
	</g:VerticalPanel>
</ui:UiBinder> 